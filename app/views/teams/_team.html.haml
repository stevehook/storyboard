%div.listItem.dragTarget{:'data-id' => team.id}
  - if can? :manage, team
    %span.listItemTitle.listItemMainTitle
      = link_to team.name, edit_team_path(team.id)
  - else
    %span.listItemTitle= team.name
  %br
  - team.members.collect do |user| 
    = render 'team_user', :user => user, :team => team
  %div.clear
