<div class='pageTitle'>Story List</div>

<table class='listTable' cellspacing='0'>
  <thead>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Description</th>
    <th>Estimate</th>
    <th>Create date</th>
    <th>Status</th>
    <th></th>
  </tr>
  </thead>

  <tbody>
<% @stories.each do |story| %>
  <tr data-href='<%= edit_story_path(story.id) %>'>
    <td><%= story.id %></td>
    <td><%= story.title %></td>
    <td><%= story.description %></td>
    <td><%= story.estimate %></td>
    <td><%= story.created_at %></td>
    <td><%= story.status.title unless story.status.nil? %></td>
  </tr>
<% end %>
  </tbody>
</table>

<br />

